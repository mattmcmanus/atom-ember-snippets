'.source.coffee':

  'Ember.Route.extend(...':
    'prefix': 'route'
    'body':
      """
        _export = Ember.Route.extend(
          model: ->
          ${2}
        )
      """



  'afterModel: ...':
    'prefix': 'route_afterModel'
    'body':
      """
        afterModel: (${1:model}, transition) ->
          ${2:#logic...}
      """



  'beforeModel: ...':
    'prefix': 'route_beforeModel'
    'body':
      """
        beforeModel: (transition) ->
          ${1:#logic...}
      """



  'setupController: ...':
    'prefix': 'route_setupController'
    'body':
      """
        setupController: (transition) ->
          ${1:#logic...}
      """
